{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "includeFiles": ["next.config.js"]
      }
    }
  ],
  "routes": [
    {
      "src": "^/about-us$",
      "dest": "/api/about-us"
    },
    {
      "handle": "filesystem"
    }
  ]
}
